{
    "logo": {
        "path": "#",
        "src": "images/logo.png",
        "alt": "blue",
        "name": "hotash"
    },
    "toggle": { 
        "open": "menu_open", 
        "close": "menu" 
    },
    "search":{
        "icon": "search",
        "placeholder": "Quick finding..." 
    },
    "widgets": [
        {
            "theme": { "light": "light_mode", "dark": "dark_mode" },
            "title": "Theme Mode",
            "addClass": "theme"
        },
        {
            "icon": "language",
            "title": "Languages",
            "addClass": "language",
            "dropdown": [
                { "flag": "images/flag/bangladesh.jpg", "name": "bengali", "check": "active" },
                { "flag": "images/flag/germany.jpg", "name": "germany" },
                { "flag": "images/flag/saudi.jpg", "name": "arabic" },
                { "flag": "images/flag/usa.jpg", "name": "english" },
                { "flag": "images/flag/sa.jpg", "name": "african" }
            ]
        },
        {
            "path": "/order-list",
            "icon": "shopping_cart", 
            "title": "Shopping",  
            "addClass": "cart",   
            "badge": { "text": "05", "variant": "primary" } 
        },
        {
            "path": "/message",
            "icon": "email",         
            "title": "Messages",  
            "addClass": "message",     
            "badge": { "text": "03", "variant": "primary" } 
        },
        {
            "path": "/notification",
            "icon": "notifications", 
            "title": "Notifications", 
            "addClass": "notify", 
            "badge": { "text": "08", "variant": "primary" } 
        }
    ],
    "profile": {
        "name": "miron mahmud",
        "role": "superadmin",
        "username": "@mironcoder",
        "email": "mironcoder@gmail.com",
        "src": "images/avatar/01.jpg",
        "alt": "user",
        "dropdown": [
            { "path": "/my-account", "icon": "person", "text": "my account" },
            { "path": "/forgot-password", "icon": "privacy_tip", "text": "reset password" },
            { "path": "/login", "icon": "lock", "text": "logout" }
        ]
    }
}